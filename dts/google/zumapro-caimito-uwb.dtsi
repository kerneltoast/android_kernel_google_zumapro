#include "google-dts-zuma/zuma-board-uwb.dtsi"

&spi_18 {
	qm35@0 {
		qm35-vdd4-supply = <&s_ldo17_reg>;
		wakeup-gpios = <&gpa5 7 GPIO_ACTIVE_HIGH>;
		ss-irq-gpios = <&gpn5 0 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>;
	};
};
